{
  "name": "frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.18"
  },
  "scripts": {
    "dev": "cross-env VITE_API_URL=http://localhost:3000/api vite --port 5173",
